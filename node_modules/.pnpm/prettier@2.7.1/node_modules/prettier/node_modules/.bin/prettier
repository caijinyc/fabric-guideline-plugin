#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/bytedance/r/fabric-guildline/node_modules/.pnpm/prettier@2.7.1/node_modules/prettier/node_modules:/Users/bytedance/r/fabric-guildline/node_modules/.pnpm/prettier@2.7.1/node_modules:/Users/bytedance/r/fabric-guildline/node_modules/.pnpm/node_modules:/Users/bytedance/r/fabric-guildline/node_modules:/Users/bytedance/r/node_modules:/Users/bytedance/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/bytedance/r/fabric-guildline/node_modules/.pnpm/prettier@2.7.1/node_modules/prettier/node_modules:/Users/bytedance/r/fabric-guildline/node_modules/.pnpm/prettier@2.7.1/node_modules:/Users/bytedance/r/fabric-guildline/node_modules/.pnpm/node_modules:/Users/bytedance/r/fabric-guildline/node_modules:/Users/bytedance/r/node_modules:/Users/bytedance/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin-prettier.js" "$@"
else
  exec node  "$basedir/../../bin-prettier.js" "$@"
fi
